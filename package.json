{
  "name": "trianglify",
  "version": "0.3.1",
  "description": "Trianglify is a javascript library for generating colorful triangle meshes that can be used as SVG images and CSS backgrounds.",
  "main": "lib/trianglify.js",
  "dependencies": {
    "chroma-js": "^1.1.1",
    "colors": "^1.1.2",
    "delaunay-fast": "^1.0.1",
    "jsdom": "^6.5.1",
    "seedrandom": "^2.3.11"
  },
  "optionalDependencies": {
    "canvas": "1.2.9"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/qrohlf/trianglify.git"
  },
  "keywords": [
    "svg",
    "d3.js",
    "visualization"
  ],
  "author": "Quinn Rohlf <qr@qrohlf.com>",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/qrohlf/trianglify/issues"
  },
  "homepage": "https://github.com/qrohlf/trianglify",
  "scripts": {
    "test": "mocha test/test.js",
    "start": "node gulpfile.js",
    "postinstall": "node scripts/postinstall.js"
  },
  "browser": {
    "jsdom": false,
    "canvas": false
  },
  "devDependencies": {
    "browserify": "^11.2.0",
    "chai": "^3.3.0",
    "del": "^2.0.2",
    "gulp": "^3.8.6",
    "gulp-bump": "^1.0.0",
    "gulp-filter": "^3.0.1",
    "gulp-git": "^1.5.0",
    "gulp-jshint": "^1.7.1",
    "gulp-mocha": "^2.1.3",
    "gulp-rename": "^1.2.0",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-tag-version": "^1.1.0",
    "gulp-uglify": "^1.4.1",
    "jshint-stylish": "^2.0.1",
    "mocha": "^2.1.0",
    "node-notifier": "^4.1.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "npmName": "trianglify",
  "npmFileMap": [
    {
      "basePath": "dist",
      "files": [
        "trianglify.min.js"
      ]
    }
  ]
